L = ["laranja", "abacate", "limao", "uva"]
if "laranja" in L:
    print("fruta incontrada")
else:
    print("fruta não esta na lista")
    
    
    L = ["Python", "Ruby", "Java", "C#"]
if "PHP" not in L:
    print("linguagem não enontrada")
else:
    print("linguagem cadastrada")


palavra = "Python"
if "a" in palavra:
    print("Letra encontrada")
else:
    print("Letra não encontrada")
