manipulador = open('arquivo.txt','r')
print("\nMetodo read():\n")
print(manipulador.read())

manipulador.seek(0)

print("\nMetodo redline():\n")
print(manipulador.readline())
print(manipulador.readline())

manipulador.seek(0)

print("\nMetodo redlines():\n")
print(manipulador.readlines())

manipulador.close()
