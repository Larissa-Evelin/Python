nota1 = 5.00
nota2 = 7.00
media = (nota1 + nota2) / 2

if media >= 7.00:
    print("Aluno aprovado %f" % media)
    print("Parabens")

else:
    if media >= 5.00:
        print("Aluno de recuperação %f" % media)

    else:
        print("aluno reprovado %f" % media)
    
print("media é %f" % media)
