# RANGE(INICIO, FIM, SALTO)

for x in range(11): # VALORES DE 0 A 10
    print(x)
   
------------------------------------------------------------   
for w in range(0, 51, 5): # VALORES DE 0 A 50 SALTANDO DE 5 EM 5
    print(w)
------------------------------------------------------------   

for w in range(0, 51): # VALORES DE 0 A 51
    print(w)
